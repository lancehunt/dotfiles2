kgpolast() {
  kubectl get pod -o go-template='{{range.status.containerStatuses}}{{"Container Name: "}}{{.name}}{{"\r\nLastState: "}}{{.lastState}}{{end}}' $1
}

ksysgpolast() {
  kubectl -n kube-system get pod -o go-template='{{range.status.containerStatuses}}{{"Container Name: "}}{{.name}}{{"\r\nLastState: "}}{{.lastState}}{{end}}' $1
}

kpods() {
  curl -s http://localhost:$1/pods/ | jq ".items[].status.phase" | sort | uniq -c
}

alias kcat='cat ~/.kubectl-profile'
alias kgrep='cat ~/.kubectl-profile | grep -i '
alias klist="kcat | sed -e 's/^alias//g' | sed -e 's/^#alias//g'"
alias kactive="kcat | grep ^alias | sed -e 's/^alias//g'"
alias kinactive="kcat | grep ^#alias | sed -e 's/^#alias//g'"

alias kns='kubens'
alias kkrew='kubectl krew'
alias kmulti="kubectl run multitool --image=praqma/network-multitool --restart Never --limits='cpu=100m,memory=200Mi' --requests='cpu=100m,memory=200Mi'"
alias kmulti-ssh="kubectl exec -it multitool -- /bin/ash"

kinterval() {
  export KUBECTL_WATCH_INTERVAL=$1
}

kset() {
  eval "export KUBECONFIG=$(pwd)/.kube-${1:-dev}"
}

kunset() {
  unset KUBECONFIG
}

eksconfig() {
  aws eks update-kubeconfig --name $1
}

ecrlogin() {
  eval "aws ecr get-login-password  | docker login --username AWS --password-stdin $1"
}

source <(kubectl completion zsh)

complete -F __start_kubectl k
complete -F __start_kubectl ksys
complete -F __start_kubectl kgpo
complete -F __start_kubectl ksysgpo
complete -F __start_kubectl kkrew
